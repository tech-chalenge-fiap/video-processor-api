{
  "name": "video-processor",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch src/main/server.ts",
    "db:generate": "prisma generate",
    "db:format": "prisma format",
    "db:seed": "prisma db seed",
    "db:migrate": "prisma migrate deploy && yarn db:generate",
    "db:create": "prisma migrate dev --create-only --name",
    "start": "yarn db:migrate && node -r module-alias/register dist/main/server.js",
    "build": "tsc -p ./tsconfig.build.json",
    "test": "jest --coverage",
    "test:coverage": "c8 yarn test",
    "test:bdd": "npx cucumber-js tests/features --require-module ts-node/register --require tests/features/stepDefinitions/**/*.ts -f @cucumber/pretty-formatter"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.738.0",
    "@prisma/client": "6.2.1",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.17.1",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "multer": "^1.4.2",
    "prisma": "^6.2.1",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.2.0",
    "@cucumber/pretty-formatter": "^1.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/multer": "^1.4.5",
    "@types/node": "^22.10.10",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "c8": "^10.1.3",
    "eslint": "^9.19.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "progress-bar": "^0.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
